PORT=3000
COMPILE_TIMEOUT_MS=30000
RUN_TIMEOUT_MS=5000
CODE_SIZE_LIMIT_BYTES=131072
CACHE_ENABLED=false
CACHE_TTL_SECONDS=60
IMAGE_GCC_HEAD=wandbox-clone-api-gcc-head
IMAGE_CLANG_HEAD=wandbox-clone-api-clang-head
WORK_DIR_IN_CONTAINER=/workspace

# Docker connection (auto-detected if not set):
#   Linux:   /var/run/docker.sock
#   macOS:   /var/run/docker.sock or ~/.docker/run/docker.sock
#   Windows: //./pipe/docker_engine
# Override with full URI: unix://, npipe://, tcp://
# DOCKER_HOST=

# Host-side Docker socket path for docker-compose volume mount (default: /var/run/docker.sock)
# DOCKER_SOCKET=/var/run/docker.sock

# Optional: path to seccomp profile (e.g. /seccomp/seccomp.json when mounted)
# SECCOMP_PROFILE_PATH=
